{
	"description" : "Speeds up browsing, by modifying response headers(cache-content etc).",
	"icons" : {
		"128" : "img/icon_enable.png"
	},
	"offline_enabled" : false,
	"background" : {
		"persistent" : true,
		"scripts" : ["js/background.js"]
	},
	"version" : "2.0",
	"options_page" : "html/option.html",
	"manifest_version" : 2,
	"developer" : {
		"name" : "mubaidr",
		"url" : "http://my.opera.com/mubaidr/blog/"
	},
	"homepage_url" : "http://my.opera.com/mubaidr/blog/",
	"permissions" : ["<all_urls>", "webRequest", "webRequestBlocking", "storage"],
	"incognito" : "spanning",
	"name" : "Speed-Up Browsing Edge"
}
